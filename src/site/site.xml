<project>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.9</version>
    </skin>

    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <topBarContainerStyle>width: 90%;</topBarContainerStyle>
            <sideBarEnabled>false</sideBarEnabled>
            <topBarIcon>
                <alt>utk003</alt>
                <src>/../images/home.png</src>
                <href>/../index-redirect.html</href>
                <title>Homepage</title>
            </topBarIcon>

            <breadcrumbDivider>Â»</breadcrumbDivider>
            <googleSearch/>

            <skipGenerationDate>true</skipGenerationDate>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
        </fluidoSkin>
    </custom>

    <publishDate position="right"/>
    <version position="right"/>

    <bannerLeft>
        <src>/../images/Utilities/proj.png</src>
        <href>/</href>
    </bannerLeft>
    <!-- Centered by script in <body.footer> -->

    <body>
        <links>
            <item name="Project GitHub" href="https://github.com/utk003/Utilities/" target="_blank"/>
        </links>

        <breadcrumbs>
            <item name="utk003" href="/../index-redirect.html"/>
            <item name="Projects" href="/../projects.html"/>
            <item name="Utilities" href="/"/>
        </breadcrumbs>

        <menu ref="parent"/>
        <menu ref="modules"/>
        <menu ref="reports"/>

        <footer>
            <![CDATA[
                <span class="copyright-msg" lead="Copyright" year="2020" author="Utkarsh Priyam">All rights reserved.</span>
                <script src="/../scripts/copyright.js"></script>

                <script type="text/javascript">
                    document.getElementById("bannerLeft").parentElement.classList.replace("pull-left", "text-center");
                    document.getElementById("bannerRight").parentElement.classList.replace("pull-right", "text-center");
                </script>
            ]]>
        </footer>
    </body>

    <poweredBy>
        <logo name="Maven" href="https://maven.apache.org/"
              img="images/logos/maven-feather.png"/>
    </poweredBy>
</project>